    </owl:NamedIndividual>
    


    <!-- http://example.org#s1 -->

    <owl:NamedIndividual rdf:about="http://example.org#s1">
        <rdf:type rdf:resource="&ssn;Sensor"/>
        <ssn:hasMeasurementCapability rdf:resource="http://example.org#mc1"/>
        <ssn:observes rdf:resource="http://example.org#p1"/>
    </owl:NamedIndividual>
